{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "tabWidth": 4
            }
        ],
        "no-console": "warn"
    }
}
